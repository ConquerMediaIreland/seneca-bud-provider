{
  "name": "@seneca/bud-provider",
  "version": "0.1.0",
  "main": "dist/bud-provider.js",
  "type": "commonjs",
  "types": "dist/bud-provider.d.ts",
  "description": "Seneca entity provider for the Bud API.",
  "homepage": "https://github.com/senecajs/seneca-bud-provider",
  "keywords": [
    "seneca",
    "bud",
    "bud-provider",
    "cms"
  ],
  "author": "Richard Rodger - richardrodger.com",
  "contributors": [
    "RÃ­ona Stokes https://github.com/rionastokes",
    "Richard Rodger - richardrodger.com"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/senecajs/seneca-bud-provider.git"
  },
  "scripts": {
    "test": "jest --coverage",
    "test-nocov": "jest --coverage",
    "test-some": "jest -t",
    "test-watch": "jest --coverage --watchAll",
    "watch": "tsc -w -d",
    "build": "tsc -d",
    "doc": "seneca-doc -p @seneca/provider",
    "clean": "rm -rf node_modules dist yarn.lock package-lock.json",
    "reset": "npm run clean && npm i && num run build && npm test",
    "repo-tag": "REPO_VERSION=`node -e \"console.log(require('./package').version)\"` && echo TAG: v$REPO_VERSION && git commit -a -m v$REPO_VERSION && git push && git tag v$REPO_VERSION && git push --tags;",
    "repo-publish": "npm run clean && npm i && npm run repo-publish-quick",
    "repo-publish-quick": "npm run build && npm run test && npm run doc && npm run repo-tag && npm publish --access public --registry https://registry.npmjs.org "
  },
  "license": "MIT",
  "files": [
    "dist",
    "src/**/*.ts",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16"
  },
  "peerDependencies": {
    "@seneca/env": ">=0.2",
    "@seneca/provider": ">=1",
    "seneca": ">=3",
    "seneca-entity": ">=24",
    "seneca-promisify": ">=3"
  },
  "devDependencies": {
    "@seneca/doc": "^5.0.1",
    "@seneca/maintain": "^0.1.0",
    "@seneca/user": "^6.3.0",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.6.0",
    "esbuild": "^0.19.2",
    "esbuild-jest": "^0.5.0",
    "jest": "^29.7.0",
    "node-fetch": "^2",
    "seneca": ">=3",
    "seneca-msg-test": "^3.4.0",
    "typescript": "^5.2.2"
  }
}
